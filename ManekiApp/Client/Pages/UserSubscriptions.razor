@page "/my-subscriptions"
@attribute [Authorize]

<PageTitle>My Subscriptions</PageTitle>
<RadzenRow>
    <RadzenColumn Size="12">
        <RadzenText Text="My Subscriptions" TextStyle="TextStyle.H3" TagName="TagName.H1" />
    </RadzenColumn>

    <RadzenRow>
        <RadzenCard class="w-100">
            <RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center">
                <RadzenImage Path="" Style="width: 100px; height: 100px; border-radius: 50%" AlternateText="" />
                <RadzenRow class="w-100">
                    <RadzenColumn class="flex-column justify-content-center" Style="display: flex">
                        <RadzenStack Gap="0">
                            <RadzenText TextStyle="TextStyle.Overline" class="rz-display-flex rz-mt-2 rz-my-0">Author Name</RadzenText>
                            <RadzenText Style="font-weight: 400"><b>Arthur</b></RadzenText>
                        </RadzenStack>
                        <RadzenStack Gap="0">
                            <RadzenText TextStyle="TextStyle.Overline" class="rz-display-flex rz-mt-2 rz-my-0">Status</RadzenText>
                            <RadzenText>Active</RadzenText>
                        </RadzenStack>
                    </RadzenColumn>
                    <RadzenColumn class="flex-column justify-content-center" Style="display: flex">
                        <RadzenStack Gap="0">
                            <RadzenText TextStyle="TextStyle.Overline" class="rz-display-flex rz-mt-2 rz-my-0">Started</RadzenText>
                            <RadzenText>21.05.2024</RadzenText>
                        </RadzenStack>
                        <RadzenStack Gap="0">
                            <RadzenText TextStyle="TextStyle.Overline" class="rz-display-flex rz-mt-4 rz-mb-0">Ends</RadzenText>
                            <RadzenText>21.06.2024</RadzenText>
                        </RadzenStack>
                    </RadzenColumn>
                    <RadzenColumn class="align-self-center" Size="5">
                        <RadzenCard Variant="Variant.Outlined">
                            <RadzenText class="rz-mb-3" Style="text-align: center; font-size: 19px; font-weight: normal">Cheap Supporter</RadzenText>
                            <RadzenBadge BadgeStyle="BadgeStyle.Info" Text="2$" Style="background: linear-gradient(90deg, rgba(58,172,180,1) 0%, rgba(29,137,253,1) 50%, rgba(119,69,252,1) 100%); height: 40px; display: flex; font-size: 25px; font-weight: 400" class="justify-content-center align-items-center" />
                        </RadzenCard>
                    </RadzenColumn>
                </RadzenRow>
            </RadzenStack>
            <hr style="border: none; background-color: rgba(0,0,0,.2); height: 1px; margin: 1rem 0;" />
            <RadzenRow>
                <RadzenColumn Size="12" class="justify-content-between" Style="display: flex">
                    <RadzenButton ButtonStyle="ButtonStyle.Danger">Cancel Subscription</RadzenButton>
                    <RadzenRow>
                        <RadzenButton ButtonStyle="ButtonStyle.Light" Icon="notifications_active" ImageAlternateText="Notifications enabled"></RadzenButton>
                        <RadzenButton ButtonStyle="ButtonStyle.Light" Icon="notifications_off" ImageAlternateText="Notifications disabled"></RadzenButton>
                    </RadzenRow>
                </RadzenColumn>
